<%= form_for @student do |f| %>
  <p>
    <%= f.label :f_name ,"first name" %>
    <%= f.text_field :f_name %>
  </p>
  <p>

  <%= f.label :l_name ,"last name"%>
  <%= f.text_field :l_name %>
  </p>
  <p>

  <%= f.label :gender %>
  <%= f.text_field :gender %>
  </p>
  <p>

  <%= f.label :email %>
  <%= f.text_field :email %>
  </p>
  <p>

  <%= f.label :mobile %>
  <%= f.text_field :mobile %>
  </p>
  <p>

  <%= f.label :linkedin %>
  <%= f.text_field :linkedin %>
  </p>
  <p>

  <%= f.label :github_user %>
  <%= f.text_field :github_user %>
  </p>
  <p>

  <%= f.label :twitter %>
  <%= f.text_field :twitter %>
  </p>
  <p>

  <%= f.label :img %>
  <%= f.text_field :img %>
  </p>
  <p>

  <%= f.label :bio %>
  <%= f.text_field :bio %>
  </p>

  <%= f.submit %>
<% end %>
